<!DOCTYPE html>
<html lang="en">

<head>
    <title>Book Collection</title>
    <link rel="stylesheet" href="/style/style.css">
</head>

<body>
    <header>
        <nav>
            <h1 class="header-nav">Best Books!</h1>
            <button class="button1"></button>
        </nav>
        <h3>Search Result</h3>
    </header>
    <main>
        <section>
            <% searchResults.forEach( book=> { %>
                <img src="<%=book.img%>">
                <h3>
                    <%= book.title %>
                </h3>
                <p>
                    <%= book.description %>
                </p>
                <% }) %>
        </section>
    </main>
</body>

</html>