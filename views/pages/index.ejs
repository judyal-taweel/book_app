<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../layout/head') %> 
</head>

<body>
    <header>
        <%- include('../layout/header') %> 
    </header>
    <main>
        <div class="serach-head">
            <h3>Search Result</h3>
            <h4>Count <%= count %></h4>
        </div>
        </div>
        <a href="/search/new">Search For Books</a>
        <% result.forEach( book=> { %>
            <div class="container">
                <img src="<%= book.imge_url %>" alt="">
                <h5><%= book.title %></h5>
                <h6><%= book.author %></h6>
                <h6>Isbn <%=book.industryIdentifiers ? book.industryIdentifiers.identifier : 'no isbn' %></h6>
                <!-- <p><%= book.description %></p> -->
                <a href="/books/<%=book.id%>">Show</a>
            </div>
        <% }) %>
    </main>
    <footer>
        <%- include('../layout/footer') %> 
    </footer>
</body>

</html>